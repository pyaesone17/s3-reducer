{
    "name": "Pyaesone17/S3Reducer",
    "type": "library",
    "description": ":package_description",
    "keywords": [
        "Pyaesone17",
        "S3Reducer"
    ],
    "homepage": "https://github.com/Pyaesone17/S3Reducer",
    "license": "MIT",
    "authors": [
        {
            "name": "Pyae Sone Nyein",
            "email": "promise286@gmail.com",
            "homepage": "https://pyaesone17.github.io/",
            "role": "Developer"
        }
    ],
    "require": {
        "illuminate/support": "~5.1",
        "php" : "~5.6|~7.0"
    },
    "require-dev": {
        "phpunit/phpunit" : ">=5.4.3",
        "squizlabs/php_codesniffer": "^2.3"
    },
    "autoload": {
        "psr-4": {
            "Pyaesone17\\S3Reducer\\": "src"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Pyaesone17\\S3Reducer\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs -p --standard=PSR2 --runtime-set ignore_errors_on_exit 1 --runtime-set ignore_warnings_on_exit 1 src tests",
        "fix-style": "phpcbf -p --standard=PSR2 --runtime-set ignore_errors_on_exit 1 --runtime-set ignore_warnings_on_exit 1 src tests"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "laravel": {
            "providers": [
                "Pyaesone17\\S3Reducer\\S3ReducerServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    }
}
